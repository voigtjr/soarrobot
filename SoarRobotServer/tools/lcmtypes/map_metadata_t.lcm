struct map_metadata_t
{
    int64_t utime;                  // microsecond time stamp

    int32_t nareas;
    double areas[nareas][4];        // x, y, w, h for each area

    int32_t ngateways;
    int16_t gateway_ids[ngateways]; // gateway id numbers
    double gateways[ngateways][2];  // x, y for each gateway
}

