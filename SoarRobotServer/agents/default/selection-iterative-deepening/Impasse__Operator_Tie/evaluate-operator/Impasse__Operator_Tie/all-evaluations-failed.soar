sp {selection*propose*all-evaluations-failed2
   (state <s> ^name selection
              ^tied-operator-count 2
              ^evaluate <e1> { <> <e1> <e2> }
             # ^quiescence t
             -^top-selection true
              ^remaining-operators 0)
   (<e1> ^symbolic-value failure)
   (<e2> ^symbolic-value failure)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name all-evaluations-failed)
}

