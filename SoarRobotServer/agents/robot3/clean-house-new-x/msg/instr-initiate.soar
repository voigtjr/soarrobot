sp {msg*propose*instr-initiate
   (state <s> ^name msg
              ^message.first <first>)
   (<first> ^word initiate
            ^next.word patrol)
-->
   (<s> ^operator <op> +)
   (<op> ^name instr-initiate
         ^type patrol)
}

sp {apply*instr-initiate
   (state <s> ^operator <o>
              ^message <m>
              ^top-state <ts>)
   (<ts> ^patrol <p>)
   (<o> ^name instr-initiate
        ^type patrol)
   (<m> ^first.next.next.word <name>)
   (<p> ^first-area <al>
        ^patrol-name <name>
        ^patrol-type <type>)
-->
   (<ts> ^current-mission <cm>)
   (<cm> ^first-area <al>
         ^name <name>
         ^type <type>
         ^status initiated)
}

sp {apply*instr-initiate*done
   (state <s> ^operator <op>
              ^superstate.operator <ss-op>)
   (<op> ^name instr-initiate)
-->
   (<ss-op> ^processed true)
}
