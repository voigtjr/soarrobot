sp {clean-house*propose*store-area
 #interrupt
   (state <s> ^name robot
              ^parameters.areas-held-in smem
              ^store)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name store-area)
}

sp {apply*store-area
   (state <s> ^operator.name store-area
              ^store <ca>
              ^smem.command <cmd>)
-->
   (<cmd> ^store <ca>)
}

sp {apply*store-area*success
   (state <s> ^operator.name store-area
              ^store <ca>
              ^smem.result.success)
-->
   (<s> ^store <ca> -)
}
