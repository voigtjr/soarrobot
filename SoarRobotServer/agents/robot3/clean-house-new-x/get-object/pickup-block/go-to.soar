sp {pickup-block*propose*go-to
   (state <s> ^name pickup-block
              ^top-state.parameters.pickup-range <r>
              ^object <o>)
   (<o> ^visible true)
  -(<s> ^object-in-range-lined-up yes
        ^io.input-link.self.stopped yes)
   (<o> ^id <id>
        ^x <x>
        ^y <y>)
-->
   (<s> ^operator <op> + =, >)
   (<op> ^name go-to
         ^destination <no>
         ^range <r>
         ^pickup-block yes)
   (<no> ^id (+ 20000 <id>)
         ^x <x>
         ^y <y>)
}
