sp {msg*propose*instr-goto
   (state <s> ^name msg
              ^message <m>)
   (<m> ^first <first>
        ^id <id>)
   (<first> ^word goto
            ^next <second>)
   (<second> ^word <area>
             ^next nil)
-->
   (<s> ^operator <op> + =)
   (<op> ^name instr-goto
         ^area <area>
         ^id <id>)
}

sp {apply*instr-goto
   (state <s> ^operator <op>
              ^superstate.operator <ss-op>)
   (<op> ^name instr-goto
         ^done true)
-->
   (<ss-op> ^processed true)
}


